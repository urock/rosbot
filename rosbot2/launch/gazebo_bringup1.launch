<launch>

  <include file="$(find rosbot2)/launch/empty_world.launch"/>
  <!-- <include file="$(find rosbot_gazebo)/launch/maze_world.launch"/> -->
  
  
  <include file="$(find rosbot_description)/launch/OneRobot.launch">
     <arg name="robot_name" value="r1" />
     <arg name="robot_position" value="-x 0.0 -y -0.5 -z 0.01 -R 0 -P 0 -Y +1.57" />
   </include>

  
  <include file="$(find rosbot_description)/launch/TwoRobot.launch">
     <arg name="robot_name" value="r2" />
     <arg name="robot_position" value="-x 0.0 -y 0.5 -z 0.01 -R 0 -P 0 -Y +1.57" />
   </include>

</launch>